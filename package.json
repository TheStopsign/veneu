    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "googleapis": "^78.0.0",
    "graphql": "^15.4.0",
    "graphql-middleware": "^6.0.10",
    "graphql-rate-limit": "^3.1.0",
    "graphql-type-json": "^0.3.2",
    "graphql-voyager": "^1.0.0-rc.31",
    "handlebars": "^4.7.7",
    "hbs": "^4.1.2",
    "jsonwebtoken": "^8.5.1",
