    "nodemailer": "^6.6.2",
    "nodemailer-express-handlebars": "^4.0.0",
    "qr-scanner": "^1.2.0",
    "quasar": "^1.15.20",
    "register-service-worker": "^1.7.2",
    "video.js": "^7.12.3",
    "videojs-constant-timeupdate": "^1.0.0",
    "videojs-youtube": "^2.6.1",
    "vue": "^2.6.14",
