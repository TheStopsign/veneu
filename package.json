{
  "name": "veneu",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service apollo:schema:generate && vue-cli-service serve --prefix .",
    "build": "vue-cli-service apollo:schema:generate && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "apollo": "vue-cli-service apollo:schema:generate && node ./apollo-server/server.js",
    "apollo:dev": "vue-cli-service apollo:schema:generate && nodemon ./apollo-server/server.js",
    "apollo:schema:generate": "vue-cli-service apollo:schema:generate",
    "apollo:schema:publish": "vue-cli-service apollo:schema:publish",
    "db": "sudo killall mongod;mongod",
    "db:clear": "node ./apollo-server/dbclear.js",
    "db:seed": "node ./apollo-server/dbseed.js",
    "start": "node ./apollo-server/server.js",
    "pretty": "prettier --write \"./**/*.{js,jsx,json,vue,gql}\""
  },
  "dependencies": {
    "@quasar/extras": "^1.10.7",
    "@quasar/quasar-ui-qcalendar": "^3.4.0",
